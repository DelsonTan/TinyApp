<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <%- include('partials/_header') %>
</head>
<body>
  <table class="table table-striped table-dark">
    <thead>
      <tr>
        <th scope="col">Short URL</th>
        <th scope="col">Long URL</th>
        <th scope="col">Edit</th>
        <th scope="col">Delete</th>
      </tr>
    </thead>
    <%for (var url in urls) {%>
    <tr>
      <td><a href="<%=urls[url].longURL%>"><%= url%></a> </td>
      <td><%= urls[url].longURL %></td>
      <td><form method="GET" action="/urls/<%= url %>">        <input type="submit" value="Edit">  </input></form></td>
      <td><form method="POST" action="/urls/<%= url %>/delete"><input type="submit" value="Delete"></input></form></td>
    </tr>
    <%}                      %>
  </table>

</body>
</html>